{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "indent": "off",
        "linebreak-style": "off",
        "semi": "off",
        "comma-dangle": "off",
        "arrow-parens": "off",
        "no-trailing-spaces": "off",
        "require-jsdoc": "off"
    },
    "extends": ["eslint:recommended", "google"],
}